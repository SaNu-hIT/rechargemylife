<script>
  function LoadDataDromDb() {
    var data = {};
    var distributorId = user_id
    data.distributorId = distributorId;
    $.ajax({
      type: 'POST',
      data: JSON.stringify(data), //input data to be sent to the server
      contentType: 'application/json; charset=utf-8',
      dataType: "json",
      url: "/shop/getshopBydistributor",
      success: function(res) {
        console.log('success');
        console.log(res);
        var statusCode = res.statusCode
        var status = res.status
        console.log("status" + status);
        console.log("statusCode" + statusCode);
        if (statusCode == "200") {
          updateDataTable(res)
        }


      },
      error: function() {
        console.log("Device control failed");
      }
    });

  }

  var mytable;

  function updateDataTable(dataAsJsonArry) {


    var shopNumber = dataAsJsonArry.data.length
    $('#shop_number').text(shopNumber);

    console.log(dataAsJsonArry.data);
    if (mytable)
      mytable.destroy();

    mytable = $('#shopsDatatable').DataTable({
      destory: true,
      "scrollX": true,
      destory: true,
      bRetrieve: true,
      searching: true,
      data: dataAsJsonArry.data,
      columns: [{
          data: "username"
        },
        {
          data: "phone"
        },
        {
          data: "password"
        },
        {
          data: "distributorId"
        },
        {
          data: "commision"
        }
      ]

    });
    console.log("Reload table");
    mytable.draw();

  }

  var user_id

  var phone

  $(document).ready(function() {
    user_id = Cookies.get('user_id');
    phone = Cookies.get('phone');
    console.log("user_id");
    console.log(user_id);
    console.log("phone");
    console.log(phone);
    $('#username_text').text(phone);
    mytable = null;
    LoadDataDromDb();
    console.log("document ready");
    $("#addshops").click(function(e) {
      e.preventDefault();
      console.log("click done");
      url = "/shop/createshops"
      console.log(url);
      var data = {};
      var username = $('#name').val();
      var password = $('#password').val();
      var phone = $('#phone').val();
      var commision = $('#commision').val();
      var pincode = $('#pincode').val();
      var distributorId = user_id
      var shopId = $('#shopId').val();
      data.username = username; //input
      data.password = password; //input
      data.phone = phone; //input
      data.distributorId = distributorId; //input
      data.pincode = pincode; //input
      data.commision = commision;
      data._id = shopId; //input
      console.log(JSON.stringify(data));
      $.ajax({
        type: 'POST',
        data: JSON.stringify(data), //input data to be sent to the server
        contentType: 'application/json; charset=utf-8',
        dataType: "json",
        url: url,
        success: function(res) {
          console.log('success');
          console.log(res);
          var statusCode = res.statusCode
          var message = res.message
          var status = res.status
          console.log("Message" + message);
          console.log("status" + status);
          console.log("statusCode" + statusCode);
          if (statusCode == "200") {
            LoadDataDromDb();
            $('#name').val("");
            $('#password').val("");
            $('#phone').val("");
            $('#commision').val("");
            $('#pincode').val("");
            swal("Success", message, "successs");
          } else {

            swal("Oops", message, "error");

          }


        },
        error: function() {
          console.log("Device control failed");
        }
      });
    });








  });
</script>









